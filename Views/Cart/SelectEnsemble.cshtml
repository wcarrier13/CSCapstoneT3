@model IEnumerable<Ensemble>
@{
    ViewData["Title"] = "Select Ensemble";
}

<h1 style="padding-top:10px; padding-bottom:10px;">Select Ensemble</h1>

<table class="table table-striped">
    <thead>
        <th>Ensemble</th>
        <th></th>
    </thead>
    <tbody>
        @foreach(Ensemble ensemble in Model)
        {
            <tr>
                <td>
                    @ensemble.EnsembleName
                </td>
                <td>
                    <a class="btn btn-info" style="width:120px" asp-action="Select" asp-route-id="@ensemble.EnsembleId">Select</a>
                </td>
            </tr>
        }
    </tbody>
</table>
