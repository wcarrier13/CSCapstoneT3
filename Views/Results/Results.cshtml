@{
    ViewData["Title"] = "Search Results";
}
<div>
    <div style="width:400px; margin-left:36%">
        <form style="width:100%; padding-bottom:30px; padding-top:30px" class="form-inline">
            <input class="form-control form-control-sm mr-3 w-75" type="text" placeholder="Search" aria-label="Search">
            <i class="fa fa-search" aria-hidden="true"></i>
        </form>
    </div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Title</th>
                <th>Composer</th>
                <th>Genre</th>
                <th>Publisher</th>
                <th>Secondary Classification</th>
                <th>In Stock</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>#</td>
                <td>#</td>
                <td>#</td>
                <td>#</td>
                <td>#</td>
                <td>#</td>
            </tr>
        </tbody>
    </table>
</div>